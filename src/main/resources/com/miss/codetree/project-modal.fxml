    <TextArea fx:id="projectAbstractField" layoutX="82.0" layoutY="146.0" prefHeight="75.0" prefWidth="436.0" />
    <Button fx:id="addProjectButton" layoutX="353.0" layoutY="338.0" mnemonicParsing="false" onMouseClicked="#addProject" text="确定" />
