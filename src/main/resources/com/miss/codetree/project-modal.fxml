<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="projectNameField" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.miss.codetree.controller.ProjectModalController">
