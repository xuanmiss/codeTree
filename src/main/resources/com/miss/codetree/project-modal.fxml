    <TextField fx:id="projectNameField" layoutX="83.0" layoutY="45.0" prefHeight="25.0" prefWidth="166.0" />
    <Label layoutX="27.0" layoutY="87.0" text="项目路径:" />
    <TextField fx:id="projectDirField" layoutX="83.0" layoutY="83.0" prefHeight="25.0" prefWidth="436.0" />
